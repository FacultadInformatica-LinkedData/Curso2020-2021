# For each of the properties, except for rdf:type, 
# how many distinct values do they take?

SELECT  ?prop COUNT (DISTINCT ?value)
  WHERE
{
  ?x rdf:type <http://dbpedia.org/ontology/Politician> .
    ?x ?prop ?value .
    filter ( ?prop not in ( rdf:type ) )
}
LIMIT 100

